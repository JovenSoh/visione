# Server name to which this instance will respond
VISIONE_SERVER_NAME=localhost
VISIONE_PORT=8000

# Credentials for administrative access to subservices (mongo-gui, mongo, tomcat, etc.)
VISIONE_USERNAME=admin
VISIONE_PASSWORD=visione

###############################################################################
### URLs of static files. The default value (http://router) will route requests
### to predefined folders in the local data root.
###

# URL prefix of full video files
VISIONE_VIDEOS_URL=http://router
# URL prefix of resized video files
VISIONE_RESIZED_VIDEOS_URL=http://router
# URL prefix of full frames
VISIONE_FRAMES_URL=http://router
# URL prefix of resized frames
VISIONE_RESIZED_FRAMES_URL=http://router


###############################################################################
### URLs of services. Default values will route requests to locally-hosted
### services.

VISIONE_ALADIN_URL=http://visione-dev.isti.cnr.it:5020/get-text-feature
VISIONE_CLIP_URL=http://visione-dev.isti.cnr.it:5010/text-to-image-search
VISIONE_CLIP_INTERNAL_SEARCH_URL=http://visione-dev.isti.cnr.it:5010/internal-image-search
VISIONE_CLIP_LAION_URL=http://visione-dev.isti.cnr.it:5030/text-to-image-search
VISIONE_CLIP_LAION_INTERNAL_SEARCH_URL=http://visione-dev.isti.cnr.it:5030/internal-image-search
